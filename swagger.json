{
    "swagger": "2.0",
    "info": {
      "title": "File Upload API",
      "version": "1.0.0"
    },
    "paths": {
      "/upload": {
        "post": {
          "summary": "Upload a file",
          "consumes": ["multipart/form-data"],
          "parameters": [
            {
              "name": "file",
              "in": "formData",
              "description": "The file to upload",
              "required": true,
              "type": "file"
            }
          ],
          "responses": {
            "200": {
              "description": "File uploaded successfully"
            }
          }
        }
      }
    }
  }
  